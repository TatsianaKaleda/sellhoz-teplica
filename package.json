        "webpack": "^5.27.1",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "bootstrap": "^4.6.0",
        "focus-visible": "^5.2.0",
        "jquery": "^3.6.0",
        "normalize.css": "^8.0.1",
